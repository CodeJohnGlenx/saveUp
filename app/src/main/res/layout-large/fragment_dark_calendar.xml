<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.cardview.widget.CardView
        app:cardBackgroundColor="@color/white"
        android:id="@+id/view_calendar"
        android:layout_width="match_parent"
        android:layout_alignParentEnd="true"
        android:background="@color/green"
        android:layout_height="1dp" />


    <com.squareup.timessquare.CalendarPickerView
        android:layout_below="@id/view_calendar"
        android:id="@+id/testCalendar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/green"
        app:tsquare_headerTextColor="@color/white"
        app:tsquare_titleTextColor="@color/white"
        app:tsquare_dayTextColor="@color/custom_dark_calendar_day_text_color"
        app:tsquare_dayBackground="@drawable/custom_calendar_dark_bg_selector"
        />



</RelativeLayout>